{
  "name": "angular-portfolio",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "serve": "yarn run update:serve && ng serve",
    "start": "yarn run start:develop",
    "start:develop": "yarn run serve --configuration develop",
    "start:production": "yarn run serve --configuration production",
    "build": "yarn run build:develop",
    "build:develop": "ng build --configuration develop",
    "build:production": "ng build --configuration production",
    "update:app": "node ./update.build.js ./src/app.json",
    "update:serve": "yarn run update:app serve",
    "update:patch": "yarn run update:app patch",
    "update:minor": "yarn run update:app minor",
    "update:major": "yarn run update:app major"
  },
  "private": true,
  "dependencies": {
